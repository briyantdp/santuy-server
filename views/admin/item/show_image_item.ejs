<div class="tab-pane fade show active" id="showImage" role="tabpanel" aria-labelledby="showImage-tab">
    <div class="row">
        <% item.imageId.map((i, index) => { %>
            <div class="col-md-6">
                <div class="card shadow mb-4">
                    <div class="card-body">
                        <img src="http://localhost:3000/<%= i.imageUrl %>" alt="imageItem" style="width: 100%; display: block;" />
                    </div>
                </div>
            </div>
        <% }) %>
    </div>
    <a href="/admin/item" class="btn btn-primary">Kembali</a>
</div>


